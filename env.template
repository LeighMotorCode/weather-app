# Weather App Environment Configuration
# Copy this file to .env and fill in your actual values

# Weatherstack API Configuration
# Get your free API key from: https://weatherstack.com/
VITE_WEATHER_API_KEY=your_api_key_here

# API Base URL (usually don't need to change this)
VITE_API_BASE_URL=http://api.weatherstack.com

# Application Environment
VITE_APP_ENV=development
